{
  "Version": "2012-10-17",
  "Id": "key-default-1",
  "Statement": [
    {
      "Sid": "Enable IAM User Permissions",
      "Effect": "Allow",
      "Principal": { "AWS": "arn:aws:iam::528757823336:root" },
      "Action": "kms:*",
      "Resource": "*"
    },
    {
      "Sid": "AllowKmsUserRole",
      "Effect": "Allow",
      "Principal": { "AWS": "arn:aws:iam::528757823336:role/week03-kms-user" },
      "Action": [
        "kms:Encrypt",
        "kms:Decrypt",
        "kms:GenerateDataKey"
      ],
      "Resource": "*"
    }
  ]
}
